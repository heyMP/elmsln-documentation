{
  "regex": [
    {
      "pattern": "<img ([\\s\\S][^>]*)>",
      "replacement": "<media-image $1></media-image>"
    },
    {
      "pattern": "(<media-image[^>]*)src([^>]*)",
      "replacement": "$1source$2"
    },
    {
      "pattern": "(<media-image[^>]*)(source=\"\/)([^>]*>)",
      "replacement": "$1source=\"$3"
    }
  ]
}